    "main": "build/lib/not-nil.js",
    "types": "build/lib/not-nil.d.ts",
    "keywords": [
        "typescript",
        "not",
        "null",
        "nil",
        "undefined",
        "nullish",
        "assert",
        "assertion",
        "unwrap",
        "optional",
        "option",
        "essentials"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Veetaha/ts-not-nil.git"
    },
    "scripts": {
