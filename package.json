    "author": "Vitaliy Kryvenko",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^24.0.18",
        "bdd-lazy-var": "^2.5.2",
        "jest": "^24.9.0",
        "npm-check-updates": "^3.1.21",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.0",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tslint": "^5.19.0",
        "typescript": "^3.7.2"
    },
    "dependencies": {
        "ts-typedefs": ">=3.1.0"
    },
    "peerDependencies": {
        "typescript": ">=3.7.x"
